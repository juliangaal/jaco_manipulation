<?xml version="1.0"?>
<launch>
    <arg name="rviz_config" default="baseline_test.rviz" />

    <node name="jaco_rviz" pkg="rviz" type="rviz" args="-d $(find jaco_manipulation)/launch/$(arg rviz_config)"/>
    <node pkg="jaco_manipulation" name="baseline_test" type="baseline_test" output="screen"/>
    <node pkg="jaco_manipulation" name="baseline_test_recorder" type="baseline_test_recorder.py" output="screen"/>
</launch>